{
  "name": "tools",
  "version": "0.0.1",
  "description": "https://tools.kanziw.dev",
  "homepage": "https://tools.kanziw.com",
  "main": "src/index.ts",
  "scripts": {
    "dev": "nodemon",
    "lint": "eslint src --ext ts",
    "prebuild": "rm -rf dist/",
    "build:js": "babel src --out-dir dist --extensions '.ts'",
    "build:types": "tsc --emitDeclarationOnly --noEmit false -d",
    "build:web": "cd web && npm run build",
    "build": "npm run build:web && npm run build:js && mv web/build dist/public",
    "start": "node dist/index.js",
    "test": "jest"
  },
  "keywords": [
    "tools"
  ],
  "author": {
    "name": "kanziw",
    "email": "kanziw@gmail.com",
    "url": "https://kanziw.dev"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/node": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-typescript": "^7.6.0",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/morgan": "^1.7.37",
    "@types/node": "^12.7.8",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.3.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "eslint": "^6.4.0",
    "eslint-import-resolver-node": "^0.3.2",
    "eslint-plugin-import-order-alphabetical": "^1.0.0",
    "eslint-plugin-jest": "^22.17.0",
    "jest": "^24.9.0",
    "nodemon": "^1.19.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "express": "^4.17.3",
    "http-status-codes": "^1.3.2",
    "morgan": "^1.9.1"
  }
}
